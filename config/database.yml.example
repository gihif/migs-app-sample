default: &default
  adapter: mysql2
  encoding: utf8mb4
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  username: root
  password: root
  host: localhost

development:
  <<: *default
  database: migs_app_sample_development

test:
  <<: *default
  database: migs_app_sample_test

production:
  <<: *default
  database: migs_app_sample_production
